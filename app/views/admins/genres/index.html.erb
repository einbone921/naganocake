<%= form_for(@genre, url:admins_genres_path) do |f| %>

<div class="form-group">
	<%= f.text_field :name %>
</div>
<div class="action">
	<%= f.submit '追加' %>
</div>
<% end %>

<table>
	<tr><th>ジャンルの名前</th></tr>
		<% @genres.each do |genre| %>

		<tr><td><%= genre.name %></td></tr>

		<% end %>
</table>